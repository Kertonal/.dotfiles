[core]
	editor = nvim
	filemode = false

[alias]
	st = status
	ci = commit
	logg = log --graph --pretty=oneline --all
	co = checkout
	br = branch
	pom = push origin master
	oops = commit --amend --no-edit
	oopsall = commit -a --amend --no-edit
	pum = pull origin master --rebase
	re = rebase
	rem = rebase master
	com = checkout master

[user]
	name = Kertonal
	email = 

[include]
    path = ~/.user.gitconfig
